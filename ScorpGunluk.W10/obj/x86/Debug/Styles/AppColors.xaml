<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
			<Color x:Key="AppBackgroundColor">#FFFFFF</Color>
            
            <Color x:Key="SectionTitleForegroundColor">#333333</Color>
            
            <Color x:Key="TitleForegroundColor">#333333</Color>
			<Color x:Key="SubtitleForegroundColor">#9f9f9f</Color>
            
            <Color x:Key="DescriptionForegroundColor">#333333</Color>
            
            <Color x:Key="NavigationPaneBackgroundColor">#e6e6e6</Color>
            <Color x:Key="NavigationPaneTextColor">#333333</Color>

            <Color x:Key="NavigationPaneButtonColor">#ce2329</Color>
            <Color x:Key="NavigationPaneButtonForegroundColor">#ffffff</Color>

            <Color x:Key="AppBarBackgroundColor">#cccccc</Color>
			<Color x:Key="AppBarForegroundColor">#ce2329</Color>
            
            <Color x:Key="BoxBackgroundColor">#e6e6e6</Color>
			<Color x:Key="BoxForegroundColor">#333333</Color>			
        </ResourceDictionary>
        <ResourceDictionary x:Key="HighContrast">
			<Color x:Key="AppBackgroundColor">#ffffff</Color>

            <Color x:Key="SectionTitleForegroundColor">#000000</Color>
            
            <Color x:Key="TitleForegroundColor">#000000</Color>
			<Color x:Key="SubtitleForegroundColor">#000000</Color>
            
            <Color x:Key="DescriptionForegroundColor">#000000</Color>
            
            <Color x:Key="NavigationPaneBackgroundColor">#000000</Color>
            <Color x:Key="NavigationPaneTextColor">#ffffff</Color>

            <Color x:Key="NavigationPaneButtonColor">#666666</Color>
            <Color x:Key="NavigationPaneButtonForegroundColor">#000000</Color>

            <Color x:Key="AppBarBackgroundColor">#000000</Color>
			<Color x:Key="AppBarForegroundColor">ffffff</Color>
            
            <Color x:Key="BoxBackgroundColor">#000000</Color>
			<Color x:Key="BoxForegroundColor">#ffffff</Color>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>

